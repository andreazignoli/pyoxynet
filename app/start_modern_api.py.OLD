#!/usr/bin/env python3
"""
Start PyOxynet Modern API (Phase 3)
Supports JSON data analysis endpoints
"""
from pyoxynet_api.api_app import create_api_app

if __name__ == '__main__':
    app = create_api_app('development')
    print("Starting PyOxynet Modern API on http://127.0.0.1:5001")
    print("API docs available at: http://127.0.0.1:5001/docs")
    print("JSON endpoint: POST /api/v1/analyze/data")
    app.run(debug=True, host='0.0.0.0', port=5001)